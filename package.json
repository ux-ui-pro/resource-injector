{
  "name": "resource-injector",
  "version": "1.3.0",
  "description": "The ResourceInjector class dynamically loads JavaScript and CSS into the webpage, ensuring each resource is loaded only once, with error handling and optional load timeouts.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/resource-injector.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/resource-injector/issues"
  },
  "homepage": "https://github.com/ux-ui-pro/resource-injector",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "vite build",
    "lint:js": "eslint **/*.{ts,js}",
    "lint:fix:js": "eslint **/*.{ts,js} --fix",
    "format:js": "prettier --write **/*.{ts,js}",
    "lint:fix": "yarn lint:fix:js && yarn format:js"
  },
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.es.js",
    "require": "./dist/index.cjs.js"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@rollup/plugin-terser": "0.4.4",
    "@types/node": "22.13.8",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "globals": "16.0.0",
    "prettier": "3.5.2",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "vite": "6.2.0",
    "vite-plugin-dts": "4.5.1"
  },
  "keywords": [
    "resource",
    "injector",
    "script",
    "stylesheet",
    "loader",
    "dynamic",
    "resource-loader",
    "script-loader",
    "style-loader",
    "async",
    "resource-injection",
    "javascript",
    "css",
    "html",
    "dynamic-loading",
    "defer",
    "promise",
    "typescript"
  ]
}
